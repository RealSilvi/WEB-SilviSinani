{
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-organize-imports"
    ],
    "tabWidth": 4,
    "semi": true,
    "bracketSameLine": false,
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "singleAttributePerLine": false,
    "tailwindFunctions": [
        "cva",
        "cn",
        "clsx"
    ]
}
